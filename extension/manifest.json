{
  "manifest_version": 3,
  "name": "Dashbord",
  "version": "1.1",
  "description": "Плагин для дашборда",
  "permissions": ["activeTab", "downloads"],
  "host_permissions": ["http://127.0.0.1:8000/*", "http://localhost:8000/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://info-bi-db.egisz.rosminzdrav.ru/*"],
      "js": ["script-loader.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["sd.js"],
      "matches": ["https://info-bi-db.egisz.rosminzdrav.ru/*"]
    }
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "action": {
    "default_icon": "img/icon48.png"
  }
}
